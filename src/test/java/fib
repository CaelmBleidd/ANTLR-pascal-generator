program test;

function return5(var a, b, c: double; e, r, t: double): integer;
begin
  a := 10.0;
  b := a + 15.0;
  e := 3;
  return5 := 5;
end;

var
  a, b, c: double;
  e, r, t: double;
  result: integer;
begin
  result := return5(a, b, c, e, r, t);
  writeln(a);
  writeln('\n');
  writeln(result);
end.
